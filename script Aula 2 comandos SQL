/* criando tabelas */
create table tabProduto(codProduto int, nomProduto text, nomFabricante text, anoProduto int);
create table tabEstoque(codEstoque int, codProdutoE int, qtdestoque int);

/* inserindo dados nas tabelas */
insert into tabProduto values(1, "sabonete", "dove", 2022), (2, "xampu", "dove", 2021), (3, "pasta de dente", "colgate", 2021);
insert into tabEstoque values(1, 1, 40), (2, 3, 50), (3, 2, 40);

/* selecionando dados */
select codProduto, nomProduto, nomFabricante from tabProduto where anoProduto = 2021;


